        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zhouhy.spring.dao.UserDao">
    <insert id="save" parameterType="user">
        insert into t_user(username,password) values (#{username},#{password}) 
    </insert>
</mapper>